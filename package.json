{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "d": "clear && yarn dev",
    "restart": "npm run clear && npm run build && npm run start",
    "clear": "clear && rm -rf .next public/sw* public/workbox*",
    "start": "next start",
    "build": "next build",
    "dev": "next dev",
    "git:clean": "git clean -fd",
    "clear-index": "cti clean src/assets/icons && cti clean src/assets/images && cti clean src/schemas",
    "create-index": "cti create src/assets/icons && cti create src/assets/images && cti create src/schemas",
    "format-index": "yarn prettier src/assets/icons/index.ts --write && yarn prettier src/assets/images/index.ts --write && yarn prettier src/schemas/index.ts --write",
    "indexes": "yarn clear-index && yarn create-index && yarn format-index"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "autoprefixer": "^10.4.16",
    "custom-env": "^2.0.2",
    "framer-motion": "^10.16.4",
    "joi": "^17.11.0",
    "next": "^13.5.4",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-cropper": "^2.3.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-query": "^3.39.3",
    "react-select": "^5.7.7",
    "react-tailwindcss-datepicker": "^1.6.6",
    "tailwind-merge": "^1.14.0",
    "tailwind-variants": "^0.1.18",
    "tldjs": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/joi": "^17.2.3",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/tldjs": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "create-ts-index": "^1.14.0",
    "eslint": "^8.51.0",
    "eslint-config-next": "^13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "schema-dts": "^1.1.2",
    "tailwindcss": "^3.3.3",
    "types-vollab": "^1.0.3",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "yarn-upgrade-all": "^0.7.2"
  },
  "packageManager": "yarn@3.5.1",
  "main": "next.config.js",
